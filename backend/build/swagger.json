		"/signup": {
			"post": {
				"operationId": "Signup",
				"responses": {
					"204": {
						"description": "No content"
					},
					"422": {
						"description": "",
						"content": {
							"application/json": {
								"schema": {
									"properties": {
										"reason": {
											"type": "string"
										}
									},
									"required": [
										"reason"
									],
									"type": "object"
								}
							}
						}
					}
				},
				"description": "Initial sign up for new users. User will have to verify email address\nby clicking the link that got sent to it.",
				"security": [],
				"parameters": [
					{
						"description": "users email address. Mustn't be present in DB for it to be accepted.",
						"in": "query",
						"name": "email",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"description": "desired username. Must be 3-16 chars and not already present in DB",
						"in": "query",
						"name": "username",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"description": "password. Minimum length of 7 chars.",
						"in": "query",
						"name": "password",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		},
